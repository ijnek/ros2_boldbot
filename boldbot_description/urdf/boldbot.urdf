<?xml version="1.0" ?>
<robot name="boldbot">
  <!-- dummy first link, directly connects with torso -->
  <link name="base_link" />

  <joint name="base_joint" type="fixed">
    <child link="torso"/>
    <parent link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>

  <link name="torso">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.035" />
      <geometry>
        <box size="0.1 0.13 0.17"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.035" />
      <geometry>
        <box size="0.1 0.13 0.17"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1"/>
      <inertia ixx="1e-3"  ixy="0.0"  ixz="0.0" iyy="1e-3" iyz="0.0" izz="1e-3" />
    </inertial>
  </link>

  <joint name="head-pan" type="continuous">
    <parent link="torso" />
    <child link="neck" />
    <origin rpy="0 0 0" xyz="0 0 0.0505" />
    <axis xyz="0 0 1" />
  </joint>

