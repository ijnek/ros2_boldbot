      <mass value="0.1"/>
      <inertia ixx="1e-4"  ixy="0.0"  ixz="0.0" iyy="1e-4" iyz="0.0" izz="1e-4" />
    </inertial>
  </link>

  <joint name="head-tilt" type="continuous">
    <parent link="neck" />
    <child link="head" />
    <origin rpy="0 0.785398163 0" xyz="0 0 0" />
    <axis xyz="0 -1 0" />
  </joint>

  <link name="head">
