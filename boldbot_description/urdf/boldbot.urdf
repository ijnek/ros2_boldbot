<?xml version="1.0" ?>
<robot name="boldbot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <!-- dummy first link, directly connects with torse -->
  <link name="base_link" />

  <joint name="base_joint" type="fixed">
    <child link="torso"/>
    <parent link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>
  
  <link name="torso">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.1 0.13 0.101"/>
      </geometry>
    </visual>
  </link>

  <joint name="head_pan_joint" type="continuous">
    <parent link="torso" />
    <child link="neck" />
    <origin rpy="0 0 0" xyz="0 0 0.0505" />
    <axis xyz="0 0 1" />
  </joint>

  <link name="neck">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>

  <joint name="head_tilt_joint" type="continuous">
    <parent link="neck" />
    <child link="head" />
    <origin rpy="0 0 0" xyz="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <link name="head">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.03"/>
      <geometry>
        <box size="0.03 0.03 0.06" />
      </geometry>
    </visual>
  </link>

  <!-- Left Arm -->
  
  <joint name="l_shoulder_pitch_joint" type="continuous">
    <parent link="torso" />
    <child link="l_shoulder" />
    <origin rpy="0 0 0" xyz="0 0.082 0" />
    <axis xyz="0 1 0" />
  </joint>

  <link name="l_shoulder">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>

  <joint name="l_shoulder_roll_joint" type="continuous">
    <parent link="l_shoulder" />
    <child link="l_upper_arm" />
    <origin rpy="0 0 0" xyz="0 0 -0.016" />
    <axis xyz="-1 0 0" />
  </joint>

  <link name="l_upper_arm">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.03" />
      <geometry>
        <box size="0.04 0.04 0.06"/>
      </geometry>
    </visual>
  </link>

  <joint name="l_elbow_joint" type="continuous">
    <parent link="l_upper_arm" />
    <child link="l_lower_arm" />
    <origin rpy="0 0 0" xyz="0.016 0 -0.06" />
    <axis xyz="0 -1 0" />
  </joint>

  <link name="l_lower_arm">
    <visual>
      <origin rpy="0 0 0" xyz="-0.016 0 -0.03" />
      <geometry>
        <box size="0.04 0.04 0.06"/>
      </geometry>
    </visual>
  </link>

  <!-- Right Arm -->

  <joint name="r_shoulder_pitch_joint" type="continuous">
    <parent link="torso" />
    <child link="r_shoulder" />
    <origin rpy="0 0 0" xyz="0 -0.082 0" />
    <axis xyz="0 -1 0" />
  </joint>

  <link name="r_shoulder">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>

  <joint name="r_shoulder_roll_joint" type="continuous">
    <parent link="r_shoulder" />
    <child link="r_upper_arm" />
    <origin rpy="0 0 0" xyz="0 0 -0.016" />
    <axis xyz="-1 0 0" />
  </joint>

  <link name="r_upper_arm">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.03" />
      <geometry>
        <box size="0.04 0.04 0.06"/>
      </geometry>
    </visual>
  </link>

  <joint name="r_elbow_joint" type="continuous">
    <parent link="r_upper_arm" />
    <child link="r_lower_arm" />
    <origin rpy="0 0 0" xyz="0.016 0 -0.06" />
    <axis xyz="0 -1 0" />
  </joint>

  <link name="r_lower_arm">
    <visual>
      <origin rpy="0 0 0" xyz="-0.016 0 -0.03" />
      <geometry>
        <box size="0.04 0.04 0.06"/>
      </geometry>
    </visual>
  </link>

  <!-- Left Leg -->
  
  <joint name="l_hip_yaw_joint" type="continuous">
    <parent link="torso" />
    <child link="l_hip_bracket" />
    <origin rpy="0 0 0" xyz="-0.005 0.037 -0.1222" />
    <axis xyz="0 0 -1" />
  </joint>

  <link name="l_hip_bracket">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>

  <joint name="l_hip_roll_joint" type="continuous">
    <parent link="l_hip_bracket" />
    <child link="l_buttock" />
    <origin rpy="0 0 0" xyz="0 0 0" />
    <axis xyz="-1 0 0" />
  </joint>

  <link name="l_buttock">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>

  <joint name="l_hip_pitch_joint" type="continuous">
    <parent link="l_buttock" />
    <child link="l_upper_leg" />
    <origin rpy="0 0 0" xyz="0 0 0" />
    <axis xyz="0 -1 0" />
  </joint>

  <link name="l_upper_leg">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.07" />
      <geometry>
        <box size="0.04 0.04 0.14"/>
      </geometry>
    </visual>
  </link>

  <joint name="l_knee_joint" type="continuous">
    <parent link="l_upper_leg" />
    <child link="l_lower_leg" />
    <origin rpy="0 0 0" xyz="0 0 -0.14" />
    <axis xyz="0 -1 0" />
  </joint>

  <link name="l_lower_leg">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.07" />
      <geometry>
        <box size="0.04 0.04 0.14"/>
      </geometry>
    </visual>
  </link>

  <joint name="l_ankle_pitch_joint" type="continuous">
    <parent link="l_lower_leg" />
    <child link="l_ankle" />
    <origin rpy="0 0 0" xyz="0 0 -0.14" />
    <axis xyz="0 1 0" />
  </joint>

  <link name="l_ankle">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>

  <joint name="l_ankle_roll_joint" type="continuous">
    <parent link="l_ankle" />
    <child link="l_foot" />
    <origin rpy="0 0 0" xyz="0 0 0" />
    <axis xyz="1 0 0" />
  </joint>

  <link name="l_foot">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.0335" />
      <geometry>
        <box size="0.12 0.05 0.02"/>
      </geometry>
    </visual>
  </link>

  <!-- Right Leg -->
  
  <joint name="r_hip_yaw_joint" type="continuous">
    <parent link="torso" />
    <child link="r_hip_bracket" />
    <origin rpy="0 0 0" xyz="-0.005 -0.037 -0.1222" />
    <axis xyz="0 0 -1" />
  </joint>

  <link name="r_hip_bracket">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>

  <joint name="r_hip_roll_joint" type="continuous">
    <parent link="r_hip_bracket" />
    <child link="r_buttock" />
    <origin rpy="0 0 0" xyz="0 0 0" />
    <axis xyz="-1 0 0" />
  </joint>

  <link name="r_buttock">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>

  <joint name="r_hip_pitch_joint" type="continuous">
    <parent link="r_buttock" />
    <child link="r_upper_leg" />
    <origin rpy="0 0 0" xyz="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <link name="r_upper_leg">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.07" />
      <geometry>
        <box size="0.04 0.04 0.14"/>
      </geometry>
    </visual>
  </link>

  <joint name="r_knee_joint" type="continuous">
    <parent link="r_upper_leg" />
    <child link="r_lower_leg" />
    <origin rpy="0 0 0" xyz="0 0 -0.14" />
    <axis xyz="0 1 0" />
  </joint>

  <link name="r_lower_leg">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.07" />
      <geometry>
        <box size="0.04 0.04 0.14"/>
      </geometry>
    </visual>
  </link>

  <joint name="r_ankle_pitch_joint" type="continuous">
    <parent link="r_lower_leg" />
    <child link="r_ankle" />
    <origin rpy="0 0 0" xyz="0 0 -0.14" />
    <axis xyz="0 -1 0" />
  </joint>

  <link name="r_ankle">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>

  <joint name="r_ankle_roll_joint" type="continuous">
    <parent link="r_ankle" />
    <child link="r_foot" />
    <origin rpy="0 0 0" xyz="0 0 0" />
    <axis xyz="1 0 0" />
  </joint>

  <link name="r_foot">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.0335" />
      <geometry>
        <box size="0.12 0.05 0.02"/>
      </geometry>
    </visual>
  </link>

</robot>
